{
  "extends": "./.frontend/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "experimentalDecorators": true,
    "target": "ES2020",
    "noImplicitAny": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "paths": {
      "@/*": [
        "backend/*"
      ],
      "~/*": [
        "frontend/*"
      ]
    }
  }
}
